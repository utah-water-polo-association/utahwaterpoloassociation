<script src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js"></script>
<link href="https://unpkg.com/leaflet@1.6.0/dist/leaflet.css" rel="stylesheet" />
<div class="not-prose">
    <div class="flex h-96">
        <div class="flex-none w-64 h-full overflow-y-scroll">
            <h2 class="text-lg font-bold">Clubs</h2>
            <ul role="list" class="divide-y divide-gray-200">
                {% for o in g.league.directory() %}
                <li class="py-4">
                    <h2>{{ o.organization.name }}</h2>
                </li>
                {%endfor %}
            </ul>
        </div>
        <div class="flex-1 w-32">
            <div id="osm-map" class="w-full h-full"></div>
        </div>
    </div>
</div>


<script>
    // Where you want to render the map.
    var element = document.getElementById('osm-map');

    // Create Leaflet map on map element.
    var map = L.map(element);

    // Add OSM tile layer to the Leaflet map.
    L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
        attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
    }).addTo(map);

    var center = L.latLng("40.65334851511579", "-111.90336901265653");
    // Target's GPS coordinates.
    var kearns = L.latLng("40.64915066074539", "-112.00678140084297");
    var olympous = L.latLng("40.6827458389517", "-111.82304984994916");
    // Set map's center to target with zoom 14.
    map.setView(center, 12);

    // Place a marker on the same location.
    L.marker(kearns).addTo(map);
    L.marker(olympous).addTo(map);
</script>